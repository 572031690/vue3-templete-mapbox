        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "typescript": "^4.8.4",
        "unplugin-auto-import": "^0.7.1",
        "unplugin-icons": "^0.14.7",
        "unplugin-vue-components": "^0.22.4",
        "vite": "^2.9.5",
        "vue-tsc": "1.0.8"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,js,vue}": [
            "eslint --fix",
            "git add ."
        ]
