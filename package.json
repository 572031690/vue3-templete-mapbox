        "@types/qs": "^6.9.7",
        "@volar/vue-language-plugin-pug": "^1.0.9",
        "@vue/compiler-sfc": "^3.2.33",
        "axios": "^1.1.3",
        "amfe-flexible": "^2.2.1",
        "autoprefixer": "^10.4.8",
        "dayjs": "^1.11.1",
        "echarts": "^5.3.2",
        "echarts-gl": "^2.0.9",
        "echarts-liquidfill": "^3.1.0",
        "element-plus": "^2.2.5",
        "file-saver": "^2.0.5",
        "js-cookie": "^3.0.1",
