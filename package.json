        "@vitejs/plugin-vue": "^2.3.1",
        "@volar/vue-language-plugin-pug": "^1.0.8",
        "eslint": "^8.23.0",
        "husky": "^7.0.0",
        "lint-staged": "^12.4.1",
        "postcss-html": "^1.5.0",
        "pug": "^3.0.2",
        "pug-plain-loader": "^1.1.0",
        "rollup-plugin-visualizer": "^5.6.0",
        "sass": "^1.53.0",
